   "source": [
    "#### 2차 풀이\n",
    "한줄 if문 사이에 한줄 for문 작성하는 법에 익숙해지자"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%% md\n"
    }
   }
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 4 2 3\n"
     ]
    }
   ],
   "source": [
    "x = int(input().split()[1])\n",
    "arr = [a for a in input().split() if int(a)<x]\n",
    "print(\" \".join(arr))"
   ],
